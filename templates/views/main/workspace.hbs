<div class="home_content">
        
    <header>
        <h2>Dashboard</h2>
        <div class="search-wrapper">
            <span class="las la-search"></span>
            <input type="search" placeholder="Search here">
        </div>
        
        <div class="user-wrapper">

            <div class="notification-wrapper">
                <div class="alerts">
                    <span><i class='bx bxs-bell' id="bell"></i></span>
                </div>
                <div class="messages">
                    <span><i class='bx bx-envelope' id="envelope"></i></span>
                </div>
            </div>

            <img src="images/profile.png" width="40px" height="40px" alt="">

            <div>
                <h4> {{user.profile.name}} </h4>
                <small> {{user.profile.description}} </small>
            </div>

        </div>
    </header>

    <div class="main">

    </div>

</div>

  